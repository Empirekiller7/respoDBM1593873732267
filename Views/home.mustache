<html>
<head>
    <link rel="stylesheet" type="text/css" href="../../css/main.css"/>
    <script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
    <link rel="stylesheet" type="text/css" href="../../css/datatables.min.css"/>
    <script type="text/javascript" src="../../js/datatables.min.js"></script>
    <link rel="stylesheet"  type="text/css" href="../../css/bootstrap.min.css" />
    <link rel="icon" type="image/png" href="../../images/favicon.png" />
    <script src="../../js/bootstrap.min.js"></script> 
    <script>
        // Data tables script
        $(document).ready( function () {
                $('#table').DataTable();
        } );
    </script> 
</head>
<body>         
<div class="content">
<div class="row">

{{ >menu }} 
<!--<div class="content-selection">-->
<div class="col-md-{{content}}" style="margin: 7px;">
<h4> {{title}} </h4>
<div id="table_{{title}}">   
    <table id="this_table" class="table table-striped table-bordered dt-responsive nowrap" style="width:100%">              
    <thead>
        <tr>             
        {{#columns}}             
            <th>{{name}}</th>             
        {{/columns}}
         <th>Details</th>             
        </tr> 
    </thead>
    <tbody>
        {{#rows}}
        <tr>                 
        {{#properties}}                 
            <td> {{value}} </td>                 
        {{/properties}}
        {{#actions}}     
                <td>         
                    <a href="{{link}}" title="{{tooltip}}" {{#events}}{{name}}="javascript: {{function}}({{args}})" {{/events}}>             
                        {{#image}}                 
                        <img src="{{src}}" alt="{{alt}}" />             
                        {{/image}}         
                    </a>     
                </td> 
            {{/actions}}               
        </tr>             
        {{/rows}}  
    </tbody>
    </table>     
</body> 
</html>